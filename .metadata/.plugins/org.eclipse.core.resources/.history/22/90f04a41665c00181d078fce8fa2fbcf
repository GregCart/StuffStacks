package testSolutions;

import java.sql.Date;

import Objects.Book;
import Objects.Image;
import Objects.Song;
import Objects.Video;

public class Usable {
	
	private Book B;
	private Song S;
	private Image I;
	private Video V;
	
	
	public Usable(Book B) {
		this.B = B;
	}
	
	public Usable(Song S) {
		this.S = S;
	}
	
	public Usable(Image I) {
		this.I = I;
	}
	
	public Usable( Video V) {
		this.V = V;
	}
	
	public Usable useBook() {
		String s1 = B.getAuthor();
		String s2 = B.getPublisher();
		String s3 = B.getSeries();
		String s4 = B.getTitle();
		Date d5 = B.getDate();
		int i6 = B.getLength();
		int i7 = B.getSeriesNum();
	}
}
